{
    "manifest_version": 3,
    "name": "Fast Access",
    "description": "An extension for open fast access page in a new tab.",
    "version": "0.0.0.1",
    "icons": {
        "16": "icons/wv16x16.png",
        "32": "icons/wv32x32.png",
        "64": "icons/wv64x64.png",
        "128": "icons/wv128x128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["activeTab", "scripting", "file://*/*"],
    "host_permissions": ["*://*/*", "file://*/*"], 
    "browser_action": {
        "default_title": "Fast Access"
    },
    "homepage_url": "https://github.com/michaelandrefraniatte"
}